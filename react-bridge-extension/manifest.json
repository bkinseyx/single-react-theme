{
  "manifest_version": 2,
  "name": "My Extension",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["http://localhost:8080/*"],
      "js": ["liferay-content.js"]
    },
    {
      "matches": ["http://localhost:3000/*"],
      "js": ["react-content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
